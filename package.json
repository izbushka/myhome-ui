{
	"name": "rpi",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"deploy": "ng build --base-href='/home/' && node ./deploy.js",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"prepare": "husky install",
		"lint": "npm run lint:js ; npm run lint:scss",
		"lint:fix": "npm run lint:js:fix ; npm run lint:scss:fix",
		"lint:js": "eslint ./src/",
		"lint:js:fix": "eslint --fix ./src/",
		"lint:scss": "npx stylelint **/*.scss",
		"lint:scss:fix": "npx stylelint --fix **/*.scss"
	},
	"private": true,
	"lint-staged": {
		"./src/**/*.{ts,js,html}": [
			"eslint --fix"
		],
		"./src/**/*.{sass,scss}": [
			"stylelint --fix"
		]
	},
	"dependencies": {
		"@angular/animations": "~12.1.0-",
		"@angular/cdk": "^12.1.3",
		"@angular/common": "~12.1.0-",
		"@angular/compiler": "~12.1.0-",
		"@angular/core": "~12.1.0-",
		"@angular/forms": "~12.1.0-",
		"@angular/material": "~12.1.3",
		"@angular/material-moment-adapter": "~12.1.3",
		"@angular/platform-browser": "~12.1.0-",
		"@angular/platform-browser-dynamic": "~12.1.0-",
		"@angular/router": "~12.1.0-",
		"@angular/service-worker": "~12.1.0-",
		"@ngneat/until-destroy": "^8.1.1",
		"@ngrx/effects": "~12.2.0",
		"@ngrx/router-store": "~12.2.0",
		"@ngrx/store": "~12.2.0",
		"angular-highcharts": "~11.3.1",
		"highcharts": "~9.0.1",
		"moment": "^2.29.1",
		"moment-timezone": "^0.5.33",
		"ngx-webstorage": "^8.0.0",
		"rxjs": "~6.6.0",
		"tslib": "^2.2.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~12.1.3",
		"@angular-eslint/builder": "12.3.1",
		"@angular-eslint/eslint-plugin": "12.3.1",
		"@angular-eslint/eslint-plugin-template": "12.3.1",
		"@angular-eslint/schematics": "12.3.1",
		"@angular-eslint/template-parser": "12.3.1",
		"@angular/cli": "~12.1.3",
		"@angular/compiler-cli": "~12.1.0-",
		"@ngrx/store-devtools": "~12.2.0",
		"@types/node": "^12.11.1",
		"@typescript-eslint/eslint-plugin": "4.28.2",
		"@typescript-eslint/parser": "4.28.2",
		"eslint": "^7.26.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "2.23.4",
		"eslint-plugin-prettier": "^3.4.0",
		"husky": "^7.0.1",
		"lint-staged": "^11.1.0",
		"prettier": "^2.3.2",
		"ssh-deploy-release": "^3.0.5",
		"stylelint": "^13.13.1",
		"stylelint-config-sass-guidelines": "^8.0.0",
		"stylelint-order": "~4.1.0",
		"typescript": "~4.3.2"
	}
}
